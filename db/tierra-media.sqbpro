CREATE VIEW 'monedas_usadas_en_atracciones' as 
select usuarios.id as 'id_usuario', sum(atracciones.price) as 'precio_atracciones'
from usuarios
join compras_usuarios CU on CU.user_id = usuarios.id
join atracciones on CU.attraction_id = atracciones.id
group by id_usuario</sql><current_tab id="0"/></tab_sql></sqlb_project>
